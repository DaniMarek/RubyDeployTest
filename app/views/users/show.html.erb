<div class="container-fluid bg-2 text-center">
	<h3><%= user.first_name %>'s Playlist</h3>
	<table class="table table-hover">
		<thead>
			<th>Artist</th>
			<th>Title</th>
			<th>Times Added</th>
		</thead>
		<tbody>
			<% @record.each do |x| %>
			<tr>
				<td><%= x.song.artist %></td>
				<td><%= x.song.title %></td>
				<td><%= x.count %></td>
			</tr>
			<% end %>
		</tbody>
	</table>
	<button class="btn"><a class="btn" href="<%= songs_path %>">Dashboard</a></button>
	<button class="btn"><a class="btn" href="/logout" data_method="delete">Logout</a></button>
</div>
